        id="approach-speed-radar" 
        type="datafield"
        name="@Strings.AppName" 
        entry="ApproachSpeedApp" 
        launcherIcon="@Drawables.LauncherIcon"
        minSdkVersion="3.2.0">
        
        <iq:products>
            <!-- Edge 1040 only for initial testing -->
            <iq:product id="edge1040"/>
        </iq:products>
        
        <iq:permissions>
            <iq:uses-permission id="Sensor"/>
